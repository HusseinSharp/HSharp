Void Main(){
  ## declear variables
  Var svar ="";
  Var d = 4; 
  Var s = 3;
  Var a = 2;

  if((d>s)&&(d>a)){
    ## the content getting executed if the statement is true
      svar = "true"; ## set the svar variable to true
  }
  else {
        ## the content getting executed if the statement is false
      svar = "false"; ## set the svar variable to false
  };

     Call svar; ## print out the result of the svar variable
};
